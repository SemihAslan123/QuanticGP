{"ast":null,"code":"import \"core-js/modules/es.date.to-string.js\";\nimport \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/web.timers.js\";\nimport homePageMap from '@/data/homePageMap.json';\nexport default {\n  data: function data() {\n    return {\n      tooltipVisible: false,\n      toolTipLogo: '',\n      toolTipName: '',\n      toolTipTextTitle: '',\n      tooltipText: '',\n      tooltipDetails: '',\n      tooltipImage: '',\n      tooltipStyles: {\n        top: '0px',\n        left: '0px'\n      },\n      currentImageMap: '/assets/homePage/CarteEvenementAvecStands.png',\n      withStands: true,\n      emplacementsPrincipaux: homePageMap.emplacementsPrincipaux,\n      emplacementsPrestataires: homePageMap.emplacementsPrestataires,\n      // Les images de la banni√®re (SECTION 1)\n      images: ['/assets/homePage/photo1.png', '/assets/homePage/photo2.png', '/assets/homePage/photo3.png'],\n      currentImageIndex: 0,\n      // Le compte √† rebours \n      targetDate: new Date('2025-07-17T07:00:00'),\n      countdown: {\n        days: 0,\n        hours: 0,\n        minutes: 0\n      },\n      startDate: new Date('2024-09-01T00:00:00')\n    };\n  },\n  computed: {\n    currentImage: function currentImage() {\n      return this.images[this.currentImageIndex];\n    }\n  },\n  mounted: function mounted() {\n    this.updateCountdown();\n    this.timer = setInterval(this.updateCountdown, 60000);\n    this.startBannerImageRotation();\n  },\n  beforeDestroy: function beforeDestroy() {\n    clearInterval(this.timer);\n    clearInterval(this.imageInterval);\n  },\n  methods: {\n    getProgressPercentage: function getProgressPercentage() {\n      var now = new Date();\n      var totalDuration = this.targetDate - this.startDate;\n      var elapsed = now - this.startDate;\n\n      // Calcul du pourcentage\n      return Math.min(100, elapsed / totalDuration * 100);\n    },\n    updateCountdown: function updateCountdown() {\n      var now = new Date();\n      var difference = this.targetDate - now;\n      if (difference <= 0) {\n        this.countdown = {\n          days: 0,\n          hours: 0,\n          minutes: 0\n        };\n        return;\n      }\n      var days = Math.floor(difference / (1000 * 60 * 60 * 24));\n      var hours = Math.floor(difference / (1000 * 60 * 60) % 24);\n      var minutes = Math.floor(difference / (1000 * 60) % 60);\n      this.countdown = {\n        days: days,\n        hours: hours,\n        minutes: minutes\n      };\n    },\n    startBannerImageRotation: function startBannerImageRotation() {\n      var _this = this;\n      this.imageInterval = setInterval(function () {\n        _this.currentImageIndex = (_this.currentImageIndex + 1) % _this.images.length;\n      }, 10000); // Changement toutes les 6 secondes\n    },\n    showTooltip: function showTooltip(info) {\n      this.toolTipLogo = info.logo;\n      this.toolTipName = info.name;\n      this.toolTipTextTitle = info.textTitle;\n      this.tooltipText = info.text;\n      this.tooltipDetails = info.details;\n      this.tooltipImage = info.image;\n      this.tooltipVisible = true;\n    },\n    hideTooltip: function hideTooltip() {\n      this.tooltipVisible = false;\n    },\n    moveTooltip: function moveTooltip(event) {\n      var scrollOffset = window.scrollY || document.documentElement.scrollTop;\n\n      // Position de l'info-bulle\n      var tooltipTop = event.clientY + scrollOffset + 40;\n      var tooltipLeft = event.clientX + 40;\n      var windowHeight = window.innerHeight;\n\n      // Hauteur estim√©e de la tooltip (ajust√©e dynamiquement si n√©cessaire)\n      var tooltipHeight = 700; // Ajuste en fonction de ta tooltip\n\n      // V√©rification si la tooltip d√©passe la hauteur de la fen√™tre\n      if (tooltipTop + tooltipHeight > windowHeight + scrollOffset) {\n        // Si oui, on la place juste avant la fin de la fen√™tre\n        tooltipTop = windowHeight + scrollOffset - tooltipHeight;\n      }\n\n      // V√©rification si la tooltip d√©passe du haut de la fen√™tre\n      if (tooltipTop < scrollOffset) {\n        // Si oui, on la place juste apr√®s le curseur\n        tooltipTop = scrollOffset + 40;\n      }\n\n      // Appliquer les nouvelles positions\n      this.tooltipStyles = {\n        top: \"\".concat(tooltipTop, \"px\"),\n        left: \"\".concat(tooltipLeft, \"px\")\n      };\n    },\n    // M√©thode pour switch entre la vue \"Avec Stands\" et \"Sans Stands\"\n    switchImage: function switchImage() {\n      if (this.withStands) {\n        this.currentImageMap = '/assets/homePage/CarteEvenementSansStands.png';\n      } else {\n        this.currentImageMap = '/assets/homePage/CarteEvenementAvecStands.png';\n      }\n      this.withStands = !this.withStands;\n    }\n  }\n};","map":{"version":3,"names":["homePageMap","data","tooltipVisible","toolTipLogo","toolTipName","toolTipTextTitle","tooltipText","tooltipDetails","tooltipImage","tooltipStyles","top","left","currentImageMap","withStands","emplacementsPrincipaux","emplacementsPrestataires","images","currentImageIndex","targetDate","Date","countdown","days","hours","minutes","startDate","computed","currentImage","mounted","updateCountdown","timer","setInterval","startBannerImageRotation","beforeDestroy","clearInterval","imageInterval","methods","getProgressPercentage","now","totalDuration","elapsed","Math","min","difference","floor","_this","length","showTooltip","info","logo","name","textTitle","text","details","image","hideTooltip","moveTooltip","event","scrollOffset","window","scrollY","document","documentElement","scrollTop","tooltipTop","clientY","tooltipLeft","clientX","windowHeight","innerHeight","tooltipHeight","concat","switchImage"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n\n   <div class=\"page-container\">\n\n      <head>\n         <meta charset=\"UTF-8\">\n      </head>\n\n    <!-- =========================================================================================================================-->\n    <!-- ================================================= PREMIER CONTAINER ==================================================== -->\n    <!-- =========================================================================================================================-->\n\n\n      <div class=\"section\" id=\"section1\">\n\n         <video class=\"video-background\" autoplay loop muted>\n\n            <source src=\"../../public/assets/homePage/vidHomePage.mp4\" type=\"video/mp4\">\n\n            <!-- Fallback message -->\n            Votre navigateur ne supporte pas les vid√©os HTML5.\n         </video>\n\n\n            <h1 class=\"nomEvenementSection1\">QUANTIC<br><span class=\"span1\">G</span>RAND <span class=\"span1\">P</span>RIX</h1>\n\n            <p class=\"sousTitreEvenementSection1\">Des pilotes <span class=\"span2\">embl√©matiques</span>, un circuit <span class=\"span2\">mythique</span>, un format <span class=\"span2\">in√©dit</span>.</p>\n\n\n         <div class=\"overlayVideo\"></div>\n\n\n      </div>\n\n\n    <!-- =========================================================================================================================-->\n    <!-- ================================================= DEUXI√àME CONTAINER =================================================== -->\n    <!-- =========================================================================================================================-->\n\n\n      <div class=\"section\" id=\"section2\">\n\n\n         <div class=\"ContainersTopSection2\">\n\n            <div class=\"firstSec2Container\">\n\n               <div class=\"calendrierContainer\">\n\n                  <p>Calendrier 2025</p>\n\n                  <div class=\"datesEvent\">\n\n                     <p class=\"ligneDate\"> \n                        <span class=\"span3\">17</span>    \n                        <span class=\"verticalDate\">Juillet <br> \n                           <span class=\"journeeCalendrier\">Premi√®re journ√©e</span>\n                        </span>\n                     </p>\n\n                     <p class=\"ligneDate\"> \n                        <span class=\"span3\">18</span>    \n                        <span class=\"verticalDate\">Juillet <br> \n                           <span class=\"journeeCalendrier\">Deuxi√®me journ√©e</span>\n                        </span>\n                     </p>\n\n                     <p class=\"ligneDate\"> \n                        <span class=\"span3\">19</span>    \n                        <span class=\"verticalDate\">Juillet <br> \n                           <span class=\"journeeCalendrier\">Troisi√®me journ√©e</span>\n                        </span>\n                     </p>\n\n                  </div>\n\n               </div>\n\n            </div>\n\n\n            <div class=\"secondSec2Container\">\n\n               <img class=\"photoMonacoNuit\" src=\"../../public/assets/homePage/monacoCircuitNuit.jpg\" alt=\"Photo Monaco\">\n\n               <p class=\"nomCircuit\">Circuit de Monaco</p>\n\n               <p class=\"lieuCircuit\">üá≤üá® Monte-Carlo, Principaut√© de Monaco</p>\n\n               <p class=\"titreCompteRebours\">Compte √† rebours</p>\n\n               <!-- COMPTE √Ä REBOURS -->\n               <div class=\"countdown-container\">\n                  <div class=\"countdown-item\">\n                     <div class=\"circle\">\n                     <div class=\"number\">{{ countdown.days }}</div>\n                     <div class=\"label\">JOURS</div>\n                     </div>\n                  </div>\n                  <div class=\"countdown-item\">\n                     <div class=\"circle\">\n                     <div class=\"number\">{{ countdown.hours }}</div>\n                     <div class=\"label\">HEURES</div>\n                     </div>\n                  </div>\n                  <div class=\"countdown-item\">\n                     <div class=\"circle\">\n                     <di